{
  "name": "ts-typekit",
  "version": "0.0.3",
  "description": "A Collection of TypeScript Utility Types",
  "license": "MIT",
  "packageManager": "yarn@4.4.0",
  "devDependencies": {
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "tsd": "^0.31.1",
    "typescript": "^5.5.4",
    "vitepress": "^1.3.2"
  },
  "scripts": {
    "format": "prettier ./source --write",
    "test:tsc": "tsc",
    "test:tsd": "tsd",
    "release": "release-it",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "typescript",
    "ts",
    "tsd",
    "types"
  ],
  "types": "./index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "source",
    "index.d.ts"
  ]
}
