{
  "name": "ts-typekit",
  "version": "0.10.0",
  "description": "A Collection of TypeScript Utility Types",
  "license": "MIT",
  "packageManager": "yarn@4.4.0",
  "workspaces": {
    "packages": [
      "docs",
      "source",
      "test-d"
    ]
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "tsd": "^0.31.1",
    "type-coverage": "^2.29.1",
    "typescript": "^5.5.4",
    "vitepress": "^1.3.2"
  },
  "scripts": {
    "format": "prettier ./source --write",
    "test:tsc": "tsc",
    "test:tsd": "tsd",
    "test:coverage": "yarn run type-coverage",
    "release": "release-it",
    "docs": "yarn workspace docs run docs:dev",
    "type-coverage": "npx type-coverage"
  },
  "keywords": [
    "typescript",
    "ts",
    "tsd",
    "types"
  ],
  "types": "./index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "source",
    "index.d.ts"
  ],
  "typeCoverage": {
    "atLeast": 99,
    "project": "tsconfig.json",
    "strict": true
  }
}
